{
    "extends": ["eslint:recommended"],
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "node": true
    },
    "rules": {
        "no-console":"error",
        "quotes": ["error", "single"],
        "object-curly-spacing": ["error", "always"],
        "comma-dangle": ["error", {
            "arrays": "only-multiline",
            "objects": "only-multiline",
            "imports": "only-multiline",
            "exports": "only-multiline",
            "functions": "never"
        }],
        "indent": ["error", 2]
    }

}